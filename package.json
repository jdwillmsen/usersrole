{
  "name": "usersrole",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "npx eslint src/**/*.{js,jsx,ts,tsx,html,css} --fix",
    "format": "npx prettier src/**/*.{js,jsx,ts,tsx,html,css,scss} --write",
    "jest": "jest",
    "ct": "ng run usersrole:ct",
    "e2e": "ng run usersrole:e2e",
    "ct:ci": "ng run usersrole:ct-ci",
    "e2e:ci": "ng run usersrole:e2e-ci",
    "jest:coverage": "jest --coverage",
    "cypress:coverage": "npx nyc report --reporter=html --reporter=lcov ---reporter=text --reporter=text-summary",
    "clean:coverage": "rm -rf coverage",
    "merge:coverage": "npx istanbul-merge --out coverage/merged-report/coverage-final.json coverage/jest/coverage-final.json coverage/cypress/coverage-final.json",
    "all:coverage": "npx nyc report --report-dir=coverage/merged-report --temp-dir=coverage/merged-report --reporter=html --reporter=lcov --reporter=text --reporter=text-summary --include=src/**/*.ts --exclude=src/main.ts --exclude=src/environments/**",
    "tests:all": "npm run jest:coverage && npm run ct:ci && npm run e2e:ci && npm run merge:coverage && npm run all:coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.4",
    "@angular/cdk": "16.0.3",
    "@angular/common": "16.0.4",
    "@angular/compiler": "16.0.4",
    "@angular/core": "16.0.4",
    "@angular/fire": "7.6.1",
    "@angular/forms": "16.0.4",
    "@angular/material": "16.0.3",
    "@angular/platform-browser": "16.0.4",
    "@angular/platform-browser-dynamic": "16.0.4",
    "@angular/router": "16.0.4",
    "@angular/service-worker": "16.0.4",
    "@types/tinycolor2": "^1.4.3",
    "ag-grid-angular": "^30.0.6",
    "ag-grid-community": "^30.0.6",
    "clean": "^4.0.2",
    "express": "4.18.2",
    "firebase": "9.22.0",
    "protobufjs": ">=7.2.4",
    "rxjs": "7.8.1",
    "semver": ">=7.5.2",
    "tinycolor2": "^1.6.0",
    "tough-cookie": ">=4.1.3",
    "tslib": "2.5.3",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-builders/jest": "^16.0.0",
    "@angular-devkit/build-angular": "16.0.5",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.0.5",
    "@angular/compiler-cli": "16.0.4",
    "@briebug/jest-schematic": "^2.1.1",
    "@cypress/code-coverage": "^3.12.1",
    "@cypress/schematic": "2.5.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "cypress": "^13.3.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "jest-preset-angular": "^13.1.1",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "webpack": "^5.88.2"
  },
  "description": "Users role and authentication template application.",
  "main": "index.js",
  "keywords": [
    "angular",
    "roles",
    "users",
    "authorization",
    "authentication"
  ],
  "author": "Jake Willmsen",
  "license": "ISC"
}
